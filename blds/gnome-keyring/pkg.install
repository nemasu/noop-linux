function postinst() {
    glib-compile-schemas /usr/share/glib-2.0/schemas
    setcap cap_ipc_lock=ep /usr/bin/gnome-keyring-daemon
}
function postup() {
    glib-compile-schemas /usr/share/glib-2.0/schemas
    setcap cap_ipc_lock=ep /usr/bin/gnome-keyring-daemon
}
function postrm() {
    glib-compile-schemas /usr/share/glib-2.0/schemas
}
